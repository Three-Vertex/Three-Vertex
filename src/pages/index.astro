---
import Layout from '../layouts/Layout.astro';
import Hero from '../components/Hero.astro';
import EditorShowcase from '../components/EditorShowcase.astro';
import Features from '../components/Features.astro';
import About from '../components/About.astro';
---

const seo = {
    title: "ThreeVertex - 3D Collaboration">
    description: "Collaborate on 3D models in real time using ThreeVertex."
    canonical: "https://threevertex.co/"
};

<Layout 
    title={seo.title}
    description={seo.description}
    canonical={seo.canonical} >

    <Hero />
    <Features />
    <EditorShowcase/>
    <About />
</Layout>